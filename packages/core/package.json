{
  "name": "@micro-frames/core",
  "version": "0.0.0",
  "description": "Iframe based micro front end framework",
  "scripts": {
    "build": "tsc --project tsconfig.json"
  },
  "keywords": [],
  "author": "anuoua",
  "license": "MIT",
  "type": "module",
  "files": [
